      <article class="single-park">
          <div class="park-name"> <a href="/parks/{{{park_name}}}">{{{park_name}}} N.P.</a> </div>
		  <div class="park-content">
		  
			{{#each thumbnails}}
				<img class="photo_slot" src = {{this}}   alt= {{{park_name}}} {{{@index}}}. ></img>
			{{/each}}
			
			<div class="review-entry"> Review {{{park_name}}} N.P. 
			<div class="rating-buttons">
				<button type="button" class={{{park_name}}} id="rank-1">1 Star</button>
				<button type="button" class={{{park_name}}} id="rank-2">2 Star</button>
				<button type="button" class={{{park_name}}} id="rank-3">3 Star</button>
				<button type="button" class={{{park_name}}} id="rank-4">4 Star</button>
				<button type="button" class={{{park_name}}} id="rank-5">5 Star</button>
			</div>
			</div>
        </div>
      </article>